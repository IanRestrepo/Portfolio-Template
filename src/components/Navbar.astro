---
    const navLinks = [
        {name: 'Home', href: '/'},
        {name: 'About', href: '/about'},
        {name: 'Projects', href: '/projects'},
        {name: 'Services', href: '/services'},
        {name: 'Contact', href: '/contact'},
    ];
---

<nav class="py-2 px-1 bg-[#061C30] border bg-opacity-60 border-slate-600 max-w-lg rounded-full m-5 mx-auto  ">
    <ul class="flex justify-between px-2">
        {
            navLinks.map((link) => (
                <li class="px-4 py-2 cursor-pointer rounded-full hover:bg-slate-700 transition-all">
                    <a href={link.href}>{link.name}</a>
                </li>
            ))
        }
    </ul>
</nav>