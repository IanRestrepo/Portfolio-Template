---
    import Navbar from '../components/Navbar.astro';
    interface Props {
        name: string;
    }

    const { name } = Astro.props;
---

<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>{ name }</title>
</head>
<body class="bg-neutral-950 text-white">
    <Navbar />
    <slot />
</body>
</html>